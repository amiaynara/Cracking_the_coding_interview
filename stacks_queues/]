"""Linked List Implementation of Queue"""

class Queue():
    def __init__(self):
        self.size   = 0
        self.head   = None
        self.tail   = None


    def is_empty(self):
        if self.head == None: 
            return True
        return False

    def enqueue(self, data):
        new_node = Node(data)
        if self.is_empty():
            self.head = new_node
            self.tail = new_node
            return 
        new_node.next = self.head.next
